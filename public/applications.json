[
  {
    "name": "home",
    "activeWhen": "^/$",
    "remoteEntry": {
      "url": "/remoteEntry.js",
      "scope": "myWorkspaceHomeApp",
      "module": "./myWorkspaceHomeApp",
      "type": "webpack"
    },
    "label": "Home",
    "route": "/",
    "type": "home"
  },
  {
    "name": "tasks",
    "activeWhen": "^/tasks(/.*)?$",
    "remoteEntry": {
      "url": "/apps/tasks/remoteEntry.js",
      "scope": "mwsTasks",
      "module": "./mwsTasks",
      "type": "webpack"
    },
    "label": "Tasks",
    "route": "/tasks",
    "type": "app"
  },
  {
    "name": "user-profile",
    "activeWhen": "^/user-profile(/.*)?$",
    "remoteEntry": {
      "url": "/apps/user-profile/remoteEntry.js",
      "scope": "mwsUserProfile",
      "module": "./mwsUserProfile",
      "type": "module"
    },
    "label": "User Profile",
    "route": "/user-profile",
    "type": "user"
  },
  {
    "name": "messages",
    "activeWhen": "^/messages(/.*)?$",
    "remoteEntry": {
      "url": "/apps/messages/assets/remoteEntry.js",
      "scope": "mwsMessages",
      "module": "./mwsMessages",
      "type": "module"
    },
    "label": "Messages",
    "route": "/messages",
    "type": "app"
  }
]
